                }
            }
        },
        "swagdto.ProductSampleData": {
            "type": "object",
            "properties": {
                "product": {
                    "$ref": "#/definitions/model.ProductDto"
                }
            }
        },
        "swagdto.ProductSampleListData": {
            "type": "object",
            "properties": {
                "products": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.ProductDto"
                    }
                }
            }
        }
    }
